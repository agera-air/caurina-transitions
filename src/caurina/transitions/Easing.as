package caurina.transitions {
    /**
     * Defines constants for an easing equation for a <code>Tweener</code> transition.
     */
    public final class Easing {
        public static const NONE: String = "easenone";
        public static const LINEAR: String = "linear";

        public static const IN_SINE: String = "easeinsine";
        public static const OUT_SINE: String = "easeoutsine";
        public static const IN_OUT_SINE: String = "easeinoutsine";
        public static const OUT_IN_SINE: String = "easeoutinsine";

        public static const IN_QUADRATIC: String = "easeinquad";
        public static const OUT_QUADRATIC: String = "easeoutquad";
        public static const IN_OUT_QUADRATIC: String = "easeinoutquad";
        public static const OUT_IN_QUADRATIC: String = "easeoutinquad";

        public static const IN_CUBIC: String = "easeincubic";
        public static const OUT_CUBIC: String = "easeoutcubic";
        public static const IN_OUT_CUBIC: String = "easeinoutcubic";
        public static const OUT_IN_CUBIC: String = "easeoutincubic";

        public static const IN_QUARTIC: String = "easeinquart";
        public static const OUT_QUARTIC: String = "easeoutquart";
        public static const IN_OUT_QUARTIC: String = "easeinoutquart";
        public static const OUT_IN_QUARTIC: String = "easeoutinquart";

        public static const IN_QUINTIC: String = "easeinquint";
        public static const OUT_QUINTIC: String = "easeoutquint";
        public static const IN_OUT_QUINTIC: String = "easeinoutquint";
        public static const OUT_IN_QUINTIC: String = "easeoutinquint";

        public static const IN_CIRCULAR: String = "easeincirc";
        public static const OUT_CIRCULAR: String = "easeoutcirc";
        public static const IN_OUT_CIRCULAR: String = "easeinoutcirc";
        public static const OUT_IN_CIRCULAR: String = "easeoutincirc";

        public static const IN_EXPONENTIAL: String = "easeinexpo";
        public static const OUT_EXPONENTIAL: String = "easeoutexpo";
        public static const IN_OUT_EXPONENTIAL: String = "easeinoutexpo";
        public static const OUT_IN_EXPONENTIAL: String = "easeoutinexpo";

        public static const IN_ELASTIC: String = "easeinelastic";
        public static const OUT_ELASTIC: String = "easeoutelastic";
        public static const IN_OUT_ELASTIC: String = "easeinoutelastic";
        public static const OUT_IN_ELASTIC: String = "easeoutinelastic";

        public static const IN_BOUNCE: String = "easeinbounce";
        public static const OUT_BOUNCE: String = "easeoutbounce";
        public static const IN_OUT_BOUNCE: String = "easeinoutbounce";
        public static const OUT_IN_BOUNCE: String = "easeoutinbounce";

        public static const IN_BACK: String = "easeinback";
        public static const OUT_BACK: String = "easeoutback";
        public static const IN_OUT_BACK: String = "easeinoutback";
        public static const OUT_IN_BACK: String = "easeoutinback";
    }
}